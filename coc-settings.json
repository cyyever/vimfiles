{
  "diagnostic.enable": true,
  "diagnostic.displayByAle": true,
  "suggest.removeDuplicateItems": true,
  "languageserver": {
    "racket-languageserver": {
      "command": "racket",
      "args": [
        "-l",
        "racket-langserver"
      ],
      "trace.server": "verbose",
      "filetypes": [
        "racket"
      ]
    }
  }
}
